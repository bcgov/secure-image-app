    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.1",
    "gulp-apidoc": "^0.2.8",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "jest": "^24.7.1",
